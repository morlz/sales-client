{
    "blocks": false,
    "brackets": "never",
    "colors": "always",
    "commaSpace": "always",
    "commentSpace": false,
    "cssLiteral": "never",
    "depthLimit": 4,
    "duplicates": true,
    "efficient": "always",
    "extendPref": "@extends",
    "globalDupe": false,
    "indentPref": 4,
    "leadingZero": "never",
    "maxErrors": 2,
    "maxWarnings": 10,
    "mixed": false,
    "namingConvention": "BEM",
    "namingConventionStrict": false,
    "none": "never",
    "noImportant": false,
    "parenSpace": "never",
    "placeholders": false,
    "prefixVarsWithDollar": "always",
    "quotePref": "single",
    "semicolons": "never",
    "sortOrder": false,
    "stackedProperties": "prefer",
    "trailingWhitespace": "never",
    "universal": false,
    "valid": true,
    "zeroUnits": "never",
    "zIndexNormalize": 10
}
